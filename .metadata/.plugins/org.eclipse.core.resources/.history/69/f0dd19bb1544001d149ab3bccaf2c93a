package com.app.lokesh;

public class Product {
	// dependencies
	private Integer pid;
	private String name;
	private String color;

	// default constructor
	public Product() {
		super();
	}

	// setter methods
	public void setPid(Integer pid) {
		this.pid = pid;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setColor(String color) {
		this.color = color;
	}

}