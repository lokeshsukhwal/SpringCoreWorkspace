package com.app.connection;

public class MyConnection {
	private String name;
	private String code;
	
	public MyConnection() {
		System.out.println("connection default constr");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	@Override
	public String toString() {
		return "MyConnection [name=" + name + ", code=" + code + "]";
	}

}
