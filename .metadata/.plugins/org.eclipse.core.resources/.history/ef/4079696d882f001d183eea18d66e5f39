package com.app.lokesh;

import org.springframework.stereotype.Component;

@Component
public class Sample {
	private int code;

	public Sample() {
		System.out.println("sample default constr");
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	@Override
	public String toString() {
		return "Sample [code=" + code + "]";
	}

}
